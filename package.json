{
    "name": "omulti",
    "author": "Vlad Vidican <vlad.vidican@gmail.com>",
    "version": "0.0.5",
    "description": "Node.js HTTP file uploads",
    "homepage": "https://github.com/VladVidican/omulti",
    "bugs": {
        "url": "https://github.com/VladVidican/omulti/issues",
        "email": "vlad.vidican@gmail.com"
    },
    "main": "index.js",
    "types": ".",
    "keywords": [
        "upload",
        "files",
        "http",
        "multipart/form-data"
    ],
    "scripts": {
        "build": "rm -rf ./dist && tsc --build lib",
        "test": "mocha",
        "bench": "ts-node ./test/benchmarks/large-single.ts && ts-node ./test/benchmarks/large-multi.ts",
        "publish": "npm run build && cp README.md ./dist && cp package.json ./dist && cp LICENSE ./dist && npm publish ./dist"
    },
    "dependencies": {
        "tiny-typed-emitter": "2.1.0"
    },
    "devDependencies": {
        "@types/mocha": "9.0.0",
        "@types/node": "16.11.6",
        "mocha": "9.1.3",
        "ts-node": "10.4.0",
        "typescript": "4.4.4"
    },
    "license": "MIT"
}
